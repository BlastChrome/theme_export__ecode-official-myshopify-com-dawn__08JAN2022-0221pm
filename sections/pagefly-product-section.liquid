  }
  if(str.toLowerCase()=="globe"){
  	str="global";
  }
  if(str.toLowerCase()=="world"){
  	str="global";
  }
  if(str.toLowerCase()=="david"){
  	str="RENAISSANCE";
  }
  if(str.toLowerCase()=="graffiti"){
  	str="i lost my head";
  }
  if(str.toLowerCase()=="tee"){
  	str="-t";
  }
  if(str.toLowerCase()=="shirt"){
  	str="-t";
  }
  
  
  
  if(str.toLowerCase()=="-t"){
  	num=2;
  }else if(str.toLowerCase().includes("era")){
  	num=3
  
  
  }else{
  	num=4;
  }

  for (i = 0; i < str.length; i++) {
      for (j = i + 1; j < str.length + 1; j++) {
      		z=str.slice(i, j)
            
            if(z.length>=num){
            	
          		result.push(z);
          	}
      }
  }
  return result;
}
















